{
    "manifest_version": 3,
    "name": "Old Portal Style",
    "version": "2.0",
    "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "options.html",
    "default_icon": "icons/icon64.png"
    },
  "icons": {
      "16": "icons/icon64.png",
      "48": "icons/icon64.png",
      "128": "icons/icon64.png"
    },
    "content_scripts": [
      {
        "matches": ["https://trevor.myschoolapp.com/lms-assignment/assignment/assignment-student-view/*"],
        "js": ["content1.js"]
      },
      {
        "matches": ["https://trevor.myschoolapp.com/lms-assignment/assignment-center/student"],
        "js": ["content2.js"]
      },
      {
        "matches": ["https://trevor.myschoolapp.com/app/student*"],
        "js": ["content3.js"]
      },{
        "matches": ["https://trevor.myschoolapp.com/app/student*"],
        "js": ["content4.js"]
      }
    ]
  }
  